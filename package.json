        "@types/node": "^12.6.8",
        "jest": "^24.8.0",
        "ts-jest": "^24.0.2",
        "ts-loader": "^6.0.4",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.5.3",
